#!/bin/sh
# this file is DEPRECATED, use bin/convert
# usage: resize $filename $width $height $target_filename $quality $other_options 

QUALITY=${5};
if [ "$QUALITY" == "" ]; then
	QUALITY=80;
fi;

umask 001;
/usr/bin/convert "$1" -colorspace RGB -depth 8 -quality $QUALITY -thumbnail $2x$3^ $6 "$4"

